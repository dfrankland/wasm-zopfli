{
  "name": "wasm-zopfli",
  "version": "1.0.2",
  "description": "ðŸ—œ WebAssembly compiled Zopfli library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dfrankland/wasm-zopfli.git"
  },
  "keywords": [
    "wasm",
    "zopfli",
    "gzip",
    "deflate",
    "zlib",
    "rust",
    "compression"
  ],
  "author": "Dylan Frankland",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dfrankland/wasm-zopfli/issues"
  },
  "homepage": "https://github.com/dfrankland/wasm-zopfli#readme",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "browser": "./dist/browser.js",
  "files": [
    "dist/index.js",
    "dist/index.mjs",
    "dist/browser.js"
  ],
  "scripts": {
    "build": "rollup -c",
    "pretest": "npm run build",
    "test": "jest",
    "lint": "eslint -c ./package.json ./*.js ./src/**/*.js ./benchmark/*.js ./benchmark/src/**/*.js"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.35",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.35",
    "@babel/preset-env": "^7.0.0-beta.35",
    "@babel/preset-stage-0": "^7.0.0-beta.35",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.4.2",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^25.5.3",
    "rollup": "^0.52.1",
    "rollup-plugin-babel": "^4.0.0-beta.0",
    "rollup-plugin-commonjs": "^8.2.3",
    "rollup-plugin-docker": "^0.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-wasm-module": "0.0.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.35",
    "kaffeerost": "0.0.1"
  },
  "babel": {
    "only": [
      "./src/**/*.js"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "stage-0"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "jest/globals": true
    },
    "rules": {
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": [
            "**/rollup.config.js",
            "**/__tests__/**/*"
          ]
        }
      ]
    },
    "plugins": [
      "jest"
    ]
  }
}
